{
	"_version": "1.8.0",
	"sap.app": {
		"_version": "1.3.0",
		"id": "com.sap.build.standard.hackathon2018",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.2.2"
		},
		"dataSources": {
			"local": {
				"uri": "/here/goes/your/serviceUrl/local/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"ach": "ach",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.32.0"
		}
	},
	"sap.ui": {
		"_version": "1.3.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.3.0",
		"rootView": {
			"viewName": "generated.app.view.Planning",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.50.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"dataSource": "local",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"loadMetadataAsync": false,
					"json": true,
					"bJSON": true,
					"defaultBindingMode": "TwoWay",
					"useBatch": true,
					"refreshAfterChange": false,
					"disableHeadRequestForToken": true
				}
			}
		},
		"resources": {
			"css": []
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.sap.build.standard.hackathon2018.view",
				"controlId": "App",
				"clearTarget": false,
				"controlAggregation": "pages",
				"bypassed": {
					"target": [
						"Overview"
					]
				}
			},
			"targets": {
				"Employee": {
					"controlAggregation": "pages",
					"viewName": "Employee",
					"viewId": "Employee",
					"viewLevel": 7,
					"transition": "slide"
				},
				"Overview": {
					"controlAggregation": "pages",
					"viewName": "Overview",
					"viewId": "Overview",
					"viewLevel": 1,
					"transition": "slide"
				},
				"Settings": {
					"controlAggregation": "pages",
					"viewName": "Settings",
					"viewId": "Settings",
					"viewLevel": 6,
					"transition": "slide"
				},
				"Carpooling": {
					"controlAggregation": "pages",
					"viewName": "Carpooling",
					"viewId": "Carpooling",
					"viewLevel": 4,
					"transition": "slide"
				},
				"CoeffCompany": {
					"controlAggregation": "pages",
					"viewName": "CoeffCompany",
					"viewId": "CoeffCompany",
					"viewLevel": 3,
					"transition": "slide"
				},
				"RewardsPage": {
					"controlAggregation": "pages",
					"viewName": "RewardsPage",
					"viewId": "RewardsPage",
					"viewLevel": 5,
					"transition": "slide"
				},
				"Planning": {
					"controlAggregation": "pages",
					"viewName": "Planning",
					"viewId": "Planning",
					"viewLevel": 2,
					"transition": "slide"
				},
				"CarpoolSubmit": {
					"controlAggregation": "pages",
					"viewName": "CarpoolSubmit",
					"viewId": "CarpoolSubmit",
					"viewLevel": 3,
					"transition": "slide"
				}
			},
			"routes": [
				{
					"pattern": "Overview/:context:",
					"name": "Overview",
					"target": [
						"Overview"
					]
				},
				{
					"pattern": "",
					"name": "default",
					"target": [
						"Overview"
					]
				},
				{
					"pattern": "Settings/:context:",
					"name": "Settings",
					"target": [
						"Settings"
					]
				},
				{
					"pattern": "Carpooling/:context:",
					"name": "Carpooling",
					"target": [
						"Carpooling"
					]
				},
				{
					"pattern": "CoeffCompany/:context:",
					"name": "CoeffCompany",
					"target": [
						"CoeffCompany"
					]
				},
				{
					"pattern": "RewardsPage/:context:",
					"name": "RewardsPage",
					"target": [
						"RewardsPage"
					]
				},
				{
					"pattern": "Planning/:context:",
					"name": "Planning",
					"target": [
						"Planning"
					]
				},
				{
					"pattern": "CarpoolSubmit/:context:",
					"name": "CarpoolSubmit",
					"target": [
						"CarpoolSubmit"
					]
				}
			]
		}
	}
}