<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.hackathon2018.controller.Planning" xmlns="sap.m"
	xmlns:commons="sap.suite.ui.commons" xmlns:core="sap.ui.core">
	<Page showHeader="true" title="Planning" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress">
		<content>
			<commons:Timeline alignment="Left" enableDoubleSided="true" axisOrientation="Vertical" width="100%" height="100%" enableBusyIndicator="true"
				enableScroll="false" forceGrowing="false" groupBy="" growingThreshold="5" showHeaderBar="false" showIcons="true" showSearch="false"
				sort="false" sortOldestFirst="false" content="{/DatumSet}">
				<commons:content>
					<commons:TimelineItem dateTime="{path: 'Date', type: 'sap.ui.model.type.Date', formatOptions: { UTC: true, style: 'short' }}" text="Test"
						userName="Achieved score" title="5" icon="sap-icon://notes"
						filterValue="{path: 'Date', type: 'sap.ui.model.type.Date', formatOptions: { UTC: true, style: 'short' }}" userNameClickable="false"
						replyCount="0" maxCharacters="0">
						<commons:embeddedControl>
							<ComboBox selectedKey="{Transport}" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
								items="{/DatumSet}">
								<items>
									<core:Item text="{Transport}" enabled="true" key="{Transport}"/>
								</items>
							</ComboBox>
						</commons:embeddedControl>
					</commons:TimelineItem>
					<commons:TimelineItem dateTime="01/01/2001" text="Text" userName="User Name"
						userPicture="/build/uilibraries/content/static/image-canvas.svg" title="Title" icon="sap-icon://notes" filterValue="Filter Value"
						userNameClickable="true" replyCount="0" maxCharacters="0">
						<commons:embeddedControl>
							<ComboBox selectedKey="{Transport}" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
								items="{/DatumSet}">
								<items>
									<core:Item text="{Transport}" enabled="true" key="{Transport}"/>
								</items>
							</ComboBox>
						</commons:embeddedControl>
					</commons:TimelineItem>
					<commons:TimelineItem dateTime="01/01/2001" text="Text" userName="User Name"
						userPicture="/build/uilibraries/content/static/image-canvas.svg" title="Title" icon="sap-icon://notes" filterValue="Filter Value"
						userNameClickable="true" replyCount="0" maxCharacters="0">
						<commons:embeddedControl>
							<ComboBox selectedKey="{Transport}" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
								items="{/DatumSet}">
								<items>
									<core:Item text="{Transport}" enabled="true" key="{Transport}"/>
								</items>
							</ComboBox>
						</commons:embeddedControl>
					</commons:TimelineItem>
					<commons:TimelineItem dateTime="01/01/2001" text="Text" userName="User Name"
						userPicture="/build/uilibraries/content/static/image-canvas.svg" title="Title" icon="sap-icon://notes" filterValue="Filter Value"
						userNameClickable="true" replyCount="0" maxCharacters="0">
						<commons:embeddedControl>
							<ComboBox selectedKey="{Transport}" editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%"
								items="{/DatumSet}">
								<items>
									<core:Item text="{Transport}" enabled="true" key="{Transport}"/>
								</items>
							</ComboBox>
						</commons:embeddedControl>
					</commons:TimelineItem>
				</commons:content>
				<commons:filterList/>
			</commons:Timeline>
		</content>
		<footer/>
		<headerContent/>
		<subHeader/>
		<customHeader/>
	</Page>
</mvc:View>